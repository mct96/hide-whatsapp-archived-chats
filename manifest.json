{
  "manifest_version": 3,
  "name": "Hide Archive in Whatsapp",
  "version": "1.0",
  "description": "Hide Archived Chats in Whatsapp",

  "icons": {
    "128": "images/block.png"
  },
  "content_scripts": [
      {
      "js": ["scripts/content.js"],
      "run_at": "document_end",
      "matches": [
        "https://web.whatsapp.com/*"
      ]
    }
  ]
}
